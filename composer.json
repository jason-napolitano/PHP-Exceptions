{
  "name": "jason-napolitano/php-exceptions",
  "type": "library",
  "description": "A small and intuitive collection of classes to assist in better exception handling inside of PHP applications",
  "homepage": "https://github.com/jason-napolitano/php-exceptions",
  "license": "MIT",
  "version": "0.0.3",
  "require": {
    "php": ">=7.3",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.2.*",
    "squizlabs/php_codesniffer": "3.4.*"
  },
  "autoload": {
    "psr-4": {
      "Exceptions\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests"
    }
  }
}
